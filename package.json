{
  "name": "calystone",
  "version": "0.5.0",
  "main": "src/index.html",
  "scripts": {
    "start": "npm run dev",
    "dev": "vite",
    "test": "npm run lint && npm run test:unit && npm run test:e2e:run",
    "test:unit": "vitest run",
    "test:e2e:open": "concurrently -s \"first\" -k \"npm run dev\" \"cypress open --e2e --browser chrome\"",
    "test:e2e:run": "concurrently -s \"first\" -k \"npm run dev\" \"cypress run --e2e\"",
    "lint": "eslint body/ cypress/ lib/ main/ view/ --ext .js,.vue",
    "build": "vite build",
    "deploy": "firebase deploy",
    "release": "release-it --ci --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joaomelo/calystone.git"
  },
  "author": "jo√£o melo <j@melo.plus> (https://joao.melo.plus)",
  "dependencies": {
    "@floating-ui/vue": "^1.0.2",
    "firebase": "^10.4.0",
    "lodash.clonedeep": "^4.5.0",
    "uuid": "^9.0.1",
    "vue": "^3.3.4",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^8.0.1",
    "@vitejs/plugin-vue": "^4.2.3",
    "concurrently": "^8.2.2",
    "cypress": "^13.6.0",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-vue": "^9.14.1",
    "firebase-tools": "^12.6.2",
    "release-it": "^17.0.0",
    "vite": "^4.3.9",
    "vitest": "^0.31.4",
    "vue-eslint-parser": "^9.3.0"
  }
}
