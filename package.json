{
  "name": "calystone",
  "version": "0.28.1",
  "main": "src/index.html",
  "type": "module",
  "scripts": {
    "start": "npm run dev",
    "dev": "vite",
    "test": "npm run lint && npm run test:unit && npm run test:e2e:run",
    "test:unit": "vitest run",
    "test:e2e:open": "concurrently -s \"first\" -k \"npm run dev\" \"npm run dev:ready && cypress open --e2e --browser chrome\"",
    "test:e2e:run": "concurrently -s \"first\" -k \"npm run dev\" \"npm run dev:ready && cypress run --e2e\"",
    "lint": "eslint --ext .js,.vue --fix body/ cypress/ lib/ main/ view/",
    "build": "vite build",
    "deploy": "firebase deploy",
    "release": "release-it --ci --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joaomelo/calystone.git"
  },
  "author": "jo√£o melo <j@melo.plus> (https://joao.melo.plus)",
  "dependencies": {
    "@codemirror/autocomplete": "^6.11.1",
    "@codemirror/commands": "^6.3.2",
    "@codemirror/lang-markdown": "^6.2.3",
    "@codemirror/language": "^6.9.3",
    "@codemirror/state": "^6.3.3",
    "@codemirror/view": "^6.22.3",
    "@primevue/themes": "^4.0.5",
    "fuse.js": "^7.0.0",
    "lodash.clonedeep": "^4.5.0",
    "primevue": "^4.0.5",
    "tailwindcss-primeui": "^0.3.4",
    "uuid": "^9.0.1",
    "vue": "^3.4.6",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@release-it/conventional-changelog": "^8.0.1",
    "@stylistic/eslint-plugin": "^2.6.4",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.5.0",
    "@vitejs/plugin-vue": "^5.0.2",
    "autoprefixer": "^10.4.20",
    "concurrently": "^8.2.2",
    "cypress": "^13.6.0",
    "eslint": "^9.9.1",
    "eslint-plugin-cypress": "^3.5.0",
    "eslint-plugin-no-only-tests": "^3.3.0",
    "eslint-plugin-perfectionist": "^3.3.0",
    "eslint-plugin-vue": "^9.27.0",
    "postcss": "^8.4.41",
    "release-it": "^17.0.0",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.0.11",
    "vitest": "^0.31.4",
    "vue-eslint-parser": "^9.4.3"
  }
}
